  author = {Sher, David J. and Pham, Nhat-Long and Shah, Jennifer L. and Sen, Neilayan and Williams, Kimberly A. and Subramaniam, Rathan M. and Moore, William and Chorley, Regina and Ahn, Chul and Khan, Saad M.},
