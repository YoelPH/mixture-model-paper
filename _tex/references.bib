@article{bhattacharya_lymph_2021-2,
  title = {Lymph Node Extracapsular Extension as a Marker of Aggressive Phenotype: {{Classification}}, Prognosis and Associated Molecular Biomarkers},
  shorttitle = {Lymph Node Extracapsular Extension as a Marker of Aggressive Phenotype},
  author = {Bhattacharya, Priyanka and Mukherjee, Rajarshi},
  year = {2021},
