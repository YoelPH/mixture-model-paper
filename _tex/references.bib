@article{souron_acute_2017,
  title = {Acute and Chronic Neuromuscular Adaptations to Local Vibration Training},
  author = {Souron, Robin and Besson, Thibault and Millet, Guillaume Y. and Lapole, Thomas},
  year = {2017},
