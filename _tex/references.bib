@article{xing_relation_2016,
  title = {Relation between the Level of Lymph Node Metastasis and Survival in Locally Advanced Head and Neck Squamous Cell Carcinoma},
  author = {Xing, Yan and Zhang, Jianjun and Lin, Heather and Gold, Kathryn A. and Sturgis, Erich M. and Garden, Adam S. and Lee, J. Jack and William Jr, William N.},
