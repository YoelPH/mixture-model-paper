  author = {Weissmann, Thomas and Mansoorian, Sina and May, Matthias Stefan and Lettmaier, Sebastian and H{\"o}fler, Daniel and Deloch, Lisa and Speer, Stefan and Balk, Matthias and Frey, Benjamin and Gaipl, Udo S. and Bert, Christoph and Distel, Luitpold Valentin and Walter, Franziska and Belka, Claus and Semrau, Sabine and Iro, Heinrich and Fietkau, Rainer and Huang, Yixing and Putz, Florian},
  year = {2023},
